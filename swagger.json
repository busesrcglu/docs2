                  - dayCounter
                  - calendar
                  - businessDayConvention
                  - maturityDayConvention
                  - dateGeneration
                  - endOfMonth
                  properties:
                    spread:
                      type: number
                      default: 0
                    frequency:
                      type: string
                      enum:
                      - NoFrequency
                      - Once
                      - Annual
                      - Semiannual
                      - EveryFourthMonth
                      - Quarterly
                      - Bimonthly
                      - Monthly
                      - EveryFourthWeek
                      - Biweekly
                      - Weekly
                      - Daily
                      - OtherFrequency
                    index:
                      type:
                      - string
                      - 'null'
                      enum:
                      - TRLibor6M
                      - TRLibor1Y
                      - Euribor1Y
                      - Euribor1M
                      - Euribor10M
                      - Euribor11M
                      - Euribor2M
                      - Euribor2W
                      - USDLiborON
                      - GBPLiborON
                      - Euribor3M
                      - Euribor4M
                      - Euribor5M
                      - Euribor6M
                      - Euribor7M
                      - Euribor8M
                      - Euribor9M
                      - EURLibor10M
                      - EURLibor11M
                      - EURLibor1M
                      - EURLibor1Y
                      - EURLibor2M
                      - EURLibor2W
                      - EURLibor3M
                      - EURLibor4M
                      - EURLibor5M
                      - EURLibor6M
                      - EURLibor7M
                      - EURLibor8M
                      - EURLibor9M
                      - EURLiborON
                    dayCounter:
                      "$ref": "#/components/schemas/dayCounter"
                    calendar:
                      "$ref": "#/components/schemas/calendar"
                    businessDayConvention:
                      type: string
                      enum:
                      - Following
                      - ModifiedFollowing
                      - Preceding
                      - ModifiedPreceding
                      - Unadjusted
                      - HalfMonthModifiedFollowing
                      - Nearest
                    maturityDayConvention:
                      type: string
                      enum:
                      - Following
                      - ModifiedFollowing
                      - Preceding
                      - ModifiedPreceding
                      - Unadjusted
                      - HalfMonthModifiedFollowing
                      - Nearest
                    dateGeneration:
                      type: string
                      enum:
                      - CDS
                      - CDS2015
                      - Backward
                      - Forward
                      - Zero
                      - ThirdWednesday
                      - Twentieth
                      - TwentiethIMM
                      - OldCDS
                    endOfMonth:
                      type: boolean
                    fixingDate:
                      type: array
                      items:
                        type:
                        - string
                        - 'null'
                        format: date
                    fixingRate:
                      type: array
                      items:
                        type:
                        - number
                        - 'null'
                        format: double
                discountCurve:
                  "$ref": "#/components/schemas/discountCurve"
                floatingLegForecastCurve:
                  "$ref": "#/components/schemas/forecastCurve"
                yieldData:
                  "$ref": "#/components/schemas/yieldData"
      responses:
        '200':
          description: Created
          content:
            application/json:
              schema:
                type: object
                properties:
                  taskId:
                    type: string
