---
openapi: 3.0.2
info:
  title: Rhoova API Documentation
  version: 1.0.2
paths:
  "/tasks/{task_id}":
    get:
      summary: Get Calculation Result
      operationId: getCalculation
      security:
      - BearerAuth:
        - read:client
      tags:
      - Calculations
      parameters:
      - name: taskId
        in: path
        description: Task id
        required: true
        schema:
          type: string
      responses:
        '200':
          description: Get Calculation Result
          content:
            application/json:
              schema:
                type: object
                properties:
                  pid:
                    type: string
                    description: Your calculation identifier
                  request_time:
                    type: string
                    description: When the calculation was created
                  result:
                    type: object
                    additionalProperties: false
                    properties:
                      data:
                        type: array
                        additionalProperties: false
                        properties:
                          effectiveness:
                            type: number
                          presentValue:
                            type: number
                          zeroRate:
                            type: number
                          discountFactor:
                            type: number
                          realizedCF:
                            type: number
                          termToMatByDay:
                            type: number
                          termToMatByYear:
                            type: number
                          accrued:
                            type: number
                          beginDate:
                            type: string
                          endDate:
                            type: string
                      totalPresentValue:
                        type: number
                      totalAccrued:
                        type: number
                      totalEffectiveness:
                        type: number
  "/tasks/{calculationName}":
    post:
      summary: Calculate Method
      description: 'RPEngineClient.core.calculate({calculationName: ''calculation-name'',data:
        {calculation parameters} })'
      operationId: calculateMethodNode
      tags:
      - nodeClient
      requestBody:
        description: Client object
        required: true
        content:
          application/json:
            schema:
              required:
              - calculationName
              - data
              type: object
              properties:
                calculationName:
                  type: string
                  description: 'Name of the calculation to create.Multi-word nouns
                    are separated by hyphens (-). For example: ''fixed-rate-bond'''
                  readOnly: true
                data:
                  type: object
                  additionalProperties: false
                  description: Calculation parameters should be added
      responses:
        '200':
          description: Created
          content:
            application/json:
              schema:
                type: object
                properties:
                  taskId:
                    type: string
                    readOnly: true
                    description: Your calculation identifier
  "/tasks/yield-curve":
    post:
      summary: Create Yield Curve Calculation
      security:
      - BearerAuth:
        - read:client
      tags:
      - Calculations
      requestBody:
        description: Client object
        required: true
        content:
          application/json:
            schema:
              type: object
              bondDataControl: true
              properties:
                valuationDate:
                  type: string
                  format: date
                recoveryRate:
                  type: number
                zeroRates:
                  type: object
                  additionalProperties: false
                  required: []
                  properties:
                    analysisDates:
                      type:
                      - array
                      - 'null'
                      items:
                        type: string
                        format: date
                    compounding:
                      type:
                      - string
                      - 'null'
                      transform:
                      - toEnumCase
                      enum:
                      - Continuous
                      - Compounded
                      - CompoundedThenSimple
                      - Simple
                      - SimpleThenCompounded
                      default: Continuous
                    frequency:
                      type:
                      - string
                      - 'null'
                      transform:
                      - toEnumCase
                      enum:
                      - NoFrequency
                      - Once
                      - Annual
                      - Semiannual
                      - EveryFourthMonth
                      - Quarterly
                      - Bimonthly
                      - Monthly
                      - EveryFourthWeek
                      - Biweekly
                      - Weekly
                      - Daily
                      - OtherFrequency
                forwardRates:
                  type: object
                  additionalProperties: false
                  required:
                  - startEndDates
                  properties:
                    startEndDates:
                      type: array
                      items:
                        type: object
                        additionalProperties: false
                        required:
                        - startDate
                        - endDate
                        properties:
                          startDate:
                            type: string
                            format: date
                          endDate:
                            type: string
                            format: date
                    compounding:
                      type:
                      - string
                      - 'null'
                      transform:
                      - toEnumCase
                      enum:
                      - Continuous
                      - Compounded
                      - CompoundedThenSimple
                      - Simple
                      - SimpleThenCompounded
                    frequency:
                      type:
                      - string
                      - 'null'
                      transform:
                      - toEnumCase
                      enum:
                      - NoFrequency
                      - Once
                      - Annual
                      - Semiannual
                      - EveryFourthMonth
                      - Quarterly
                      - Bimonthly
                      - Monthly
                      - EveryFourthWeek
                      - Biweekly
                      - Weekly
                      - Daily
                      - OtherFrequency
                discountRates:
                  type: object
                  additionalProperties: false
                  required:
                  - analysisDates
                  properties:
                    analysisDates:
                      type: array
                      items:
                        type: string
                        format: date
                defaultProbabilities:
                  type: object
                  additionalProperties: false
                  required:
                  - analysisDates
                  properties:
                    analysisDates:
                      type: array
                      items:
                        type:
                        - string
                        - 'null'
                        format: date
                survivalProbabilities:
                  type: object
                  additionalProperties: false
                  required:
                  - analysisDates
                  properties:
                    analysisDates:
                      type: array
                      items:
                        type:
                        - string
                        - 'null'
                        format: date
                hazardRates:
                  type: object
                  additionalProperties: false
                  required:
                  - analysisDates
                  properties:
                    analysisDates:
                      type: array
                      items:
                        type:
                        - string
                        - 'null'
                        format: date
                yieldCurve:
                  allOf:
                  - applyShockControl: true
                    type: object
                    additionalProperties: false
                    required:
                    - settlementDays
                    - intpMethod
                    - currency
                    - instruments
                    properties:
                      settlementDays:
                        type: number
                      intpMethod:
                        type: string
                        transform:
                        - toEnumCase
                        enum:
                        - Linear
                        - LogLinear
                        - Cubic
                        - LogCubic
                      currency:
                        "$ref": "#/components/schemas/currency"
                      calendar:
                        "$ref": "#/components/schemas/calendar"
                      dayCounter:
                        "$ref": "#/components/schemas/dayCounter"
                      period:
                        type: string
                        periodControl: true
                        description: 'It should contain numbers and ''D'',''W'',''M'',''Y''.
                          For example: ''3M''. If the instrument value is ''FRA'',
                          it must contain the tenor ''X''.'
                      applyShock:
                        oneOf:
                        - type: array
                          items:
                            type: object
                            additionalProperties: false
                            required:
                            - tenor
                            - shockValue
                            properties:
                              tenor:
                                type: string
                              shockValue:
                                type: number
                                format: double
                        - type: object
                          additionalProperties: false
                          required:
                          - method
                          - shockValues
                          properties:
                            method:
                              type: string
                              transform:
                              - toEnumCase
                              enum:
                              - parallel
                              - nonparallel
                              - keyrate
                            shockValues:
                              type: array
                              items:
                                type: object
                                properties:
                                  tenor:
                                    type: string
                                  shockValue:
                                    type: number
                          if:
                            method:
                              enum:
                              - parallel
                          then:
                            shockValues:
                              tenor:
                                enum:
                                - all
                                maxLength: '1'
                      endOfMonth:
                        type: boolean
                        default: 'true'
                  - oneOf:
                    - properties:
                        instruments:
                          allOf:
                          - "$ref": "#/components/schemas/yieldCurveInstruments"
                          - anyOf:
                            - description: If this field exists, it must be baseCurve
                                and discountCurve fields
                              properties:
                                BSSWP:
                                  "$ref": "#/components/schemas/BSSWP"
                              required:
                              - BSSWP
                    - properties:
                        discountRates:
                          "$ref": "#/components/schemas/discountRates"
                    - properties:
                        zeroRates:
                          "$ref": "#/components/schemas/zeroRates"
                discountCurve:
                  allOf:
                  - description: This field is required if yieldCurve has BSSWP in
                      it
                  - "$ref": "#/components/schemas/discountCurve"
                baseCurve:
                  allOf:
                  - description: This field is required if yieldCurve has BSSWP in
                      it
                  - "$ref": "#/components/schemas/yieldCurve"
                creditCurve:
                  "$ref": "#/components/schemas/creditCurve"
                yieldData:
                  "$ref": "#/components/schemas/yieldData"
              if:
                properties:
                  creditCurve:
                    not:
                      maxLength: 0
              then:
                required:
                - valuationDate
                - yieldCurve
                - yieldData
              else:
                required:
                - valuationDate
                - yieldCurve
                - yieldData
                - recoveryRate
      responses:
        '200':
          description: Created
          content:
            application/json:
              schema:
                type: object
                properties:
                  taskId:
                    type: string
  "/tasks/interest-rates-swap":
