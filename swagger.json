                      type: string
                      transform:
                      - toEnumCase
                      enum:
                      - Following
                      - ModifiedFollowing
                      - Preceding
                      - ModifiedPreceding
                      - Unadjusted
                      - HalfMonthModifiedFollowing
                      - Nearest
                    dateGeneration:
                      type: string
                      transform:
                      - toEnumCase
                      enum:
                      - Backward
                      - Forward
                      - Zero
                      - ThirdWednesday
                      - Twentieth
                      - TwentiethIMM
                      - OldCDS
                      - CDS
